<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextMaps" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">@color/cupos_dark_gray</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@color/white</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="TextLoginTitle" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">42sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextLoginSlogan" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
    </style>


</resources>